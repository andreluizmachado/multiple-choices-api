{
    "name": "oat/multiple-choice-api",
    "description": "Multiple choice api test",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "justinrainbow/json-schema": "^5.2",
        "monolog/monolog": "^1.17",
        "slim/slim": "^3.1",
        "stichoza/google-translate-php": "^4.0",
        "vlucas/phpdotenv": "^4.1"
    },
    "require-dev": {
        "ext-dom": "*",
        "phpunit/phpunit": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "OAT\\MultipleChoiceApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
